<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  id="Definitions_1"
                  targetNamespace="http://bpmn.io/schema/bpmn"
                  exporter="Transformation Consultant Agent - BPMN Generation Skill"
                  exporterVersion="1.0">

  <bpmn:process id="Process_EmployeeOnboarding" name="Employee Onboarding Process" isExecutable="false">

    <!-- Swimlanes -->
    <bpmn:laneSet id="LaneSet_1">
      <bpmn:lane id="Lane_HRCoordinator" name="HR Coordinator">
        <bpmn:flowNodeRef>StartEvent_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_1_InitiateNewHire</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_2_ConductBackgroundCheck</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_1_BackgroundResult</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_4_SetUpEmployeeRecord</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_2_EmploymentType</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_5_ProvisionITAccessEquipment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_3_WorkLocation</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_7_ConductOrientation</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_8_CompleteComplianceTraining</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_9_PerformCheckIns</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_1</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_OfferRescinded</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_ContractorOnboarding</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_HRDirector" name="HR Director">
        <bpmn:flowNodeRef>Activity_3_ReviewBackgroundIssues</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_4_BackgroundDecision</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_ITDepartment" name="IT Department">
        <bpmn:flowNodeRef>Activity_5a_ProvisionRemoteEquipment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_5b_ProvisionOnsiteEquipment</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Facilities" name="Facilities Department">
        <bpmn:flowNodeRef>Activity_6_AssignWorkspace</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_5_WorkspaceJoin</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_HiringManager" name="Hiring Manager">
        <bpmn:flowNodeRef>Gateway_6_OrientationAlignment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_7a_ManagerFirstDayOrientation</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_NewHire" name="New Hire">
        <bpmn:flowNodeRef>Gateway_7_PreChecklistComplete</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Activity_10_CompleteRemainingPaperwork</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_8_ITAccessStatus</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>

    <!-- Start Event -->
    <bpmn:startEvent id="StartEvent_1" name="Signed Offer Letter Received">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>

    <!-- Activity 1: Initiate New Hire Process (APQC 4.1.1) -->
    <bpmn:task id="Activity_1_InitiateNewHire" name="4.1.1 Initiate New Hire Process">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>

    <!-- Activity 2: Conduct Background Verification (APQC 4.1.2) -->
    <bpmn:task id="Activity_2_ConductBackgroundCheck" name="4.1.2 Conduct Background Verification">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>

    <!-- Decision Point 1: Background Check Results -->
    <bpmn:exclusiveGateway id="Gateway_1_BackgroundResult" name="Background Check Clean?">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4_Clean</bpmn:outgoing>
      <bpmn:outgoing>Flow_5_Issues</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- Activity 3: Review Background Issues (Escalation Path) -->
    <bpmn:task id="Activity_3_ReviewBackgroundIssues" name="Review Background Check Issues">
      <bpmn:incoming>Flow_5_Issues</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:task>

    <!-- Decision Point: Background Decision -->
    <bpmn:exclusiveGateway id="Gateway_4_BackgroundDecision" name="Proceed with Hire?">
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7_Proceed</bpmn:outgoing>
      <bpmn:outgoing>Flow_8_Rescind</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- Activity 4: Set Up Employee Record (APQC 4.1.3) -->
    <bpmn:task id="Activity_4_SetUpEmployeeRecord" name="4.1.3 Set Up Employee Record">
      <bpmn:incoming>Flow_4_Clean</bpmn:incoming>
      <bpmn:incoming>Flow_7_Proceed</bpmn:incoming>
      <bpmn:outgoing>Flow_9</bpmn:outgoing>
    </bpmn:task>

    <!-- Decision Point 2: Employment Type -->
    <bpmn:exclusiveGateway id="Gateway_2_EmploymentType" name="Employment Type?">
      <bpmn:incoming>Flow_9</bpmn:incoming>
      <bpmn:outgoing>Flow_10_FullTime</bpmn:outgoing>
      <bpmn:outgoing>Flow_11_Contractor</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- Activity 5: Provision IT Access and Equipment (APQC 4.1.4) -->
    <bpmn:task id="Activity_5_ProvisionITAccessEquipment" name="4.1.4 Provision IT Access and Equipment">
      <bpmn:incoming>Flow_10_FullTime</bpmn:incoming>
      <bpmn:outgoing>Flow_12</bpmn:outgoing>
    </bpmn:task>

    <!-- Decision Point 3: Work Location -->
    <bpmn:exclusiveGateway id="Gateway_3_WorkLocation" name="Work Location?">
      <bpmn:incoming>Flow_12</bpmn:incoming>
      <bpmn:outgoing>Flow_13_Remote</bpmn:outgoing>
      <bpmn:outgoing>Flow_14_Onsite</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- Activity 5a: Provision Remote Equipment (IT Lane) -->
    <bpmn:task id="Activity_5a_ProvisionRemoteEquipment" name="Ship Complete Equipment Setup">
      <bpmn:incoming>Flow_13_Remote</bpmn:incoming>
      <bpmn:outgoing>Flow_15</bpmn:outgoing>
    </bpmn:task>

    <!-- Activity 5b: Provision Onsite Equipment (IT Lane) -->
    <bpmn:task id="Activity_5b_ProvisionOnsiteEquipment" name="Prepare Onsite Equipment">
      <bpmn:incoming>Flow_14_Onsite</bpmn:incoming>
      <bpmn:outgoing>Flow_16</bpmn:outgoing>
    </bpmn:task>

    <!-- Activity 6: Assign Workspace (APQC 4.1.5) -->
    <bpmn:task id="Activity_6_AssignWorkspace" name="4.1.5 Assign Workspace">
      <bpmn:incoming>Flow_16</bpmn:incoming>
      <bpmn:outgoing>Flow_17</bpmn:outgoing>
    </bpmn:task>

    <!-- Gateway 5: Workspace Join (Merge Remote and Onsite Paths) -->
    <bpmn:exclusiveGateway id="Gateway_5_WorkspaceJoin" name="">
      <bpmn:incoming>Flow_15</bpmn:incoming>
      <bpmn:incoming>Flow_17</bpmn:incoming>
      <bpmn:outgoing>Flow_18</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- Decision Point 4: Orientation Alignment -->
    <bpmn:exclusiveGateway id="Gateway_6_OrientationAlignment" name="Start Date on Orientation Monday?">
      <bpmn:incoming>Flow_18</bpmn:incoming>
      <bpmn:outgoing>Flow_19_OrientationDay</bpmn:outgoing>
      <bpmn:outgoing>Flow_20_NonOrientationDay</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- Activity 7: Conduct Orientation (APQC 4.1.6) -->
    <bpmn:task id="Activity_7_ConductOrientation" name="4.1.6 Conduct Orientation">
      <bpmn:incoming>Flow_19_OrientationDay</bpmn:incoming>
      <bpmn:incoming>Flow_21</bpmn:incoming>
      <bpmn:outgoing>Flow_22</bpmn:outgoing>
    </bpmn:task>

    <!-- Activity 7a: Manager First-Day Orientation -->
    <bpmn:task id="Activity_7a_ManagerFirstDayOrientation" name="Manager First-Day Orientation">
      <bpmn:incoming>Flow_20_NonOrientationDay</bpmn:incoming>
      <bpmn:outgoing>Flow_21</bpmn:outgoing>
    </bpmn:task>

    <!-- Decision Point 6: Pre-Checklist Complete -->
    <bpmn:exclusiveGateway id="Gateway_7_PreChecklistComplete" name="Pre-Orientation Checklist Complete?">
      <bpmn:incoming>Flow_22</bpmn:incoming>
      <bpmn:outgoing>Flow_23_Complete</bpmn:outgoing>
      <bpmn:outgoing>Flow_24_Incomplete</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- Activity 10: Complete Remaining Paperwork -->
    <bpmn:task id="Activity_10_CompleteRemainingPaperwork" name="4.1.7 Enroll in Benefits">
      <bpmn:incoming>Flow_24_Incomplete</bpmn:incoming>
      <bpmn:outgoing>Flow_25</bpmn:outgoing>
    </bpmn:task>

    <!-- Decision Point 5: IT Access Status -->
    <bpmn:exclusiveGateway id="Gateway_8_ITAccessStatus" name="IT Systems Accessible?">
      <bpmn:incoming>Flow_23_Complete</bpmn:incoming>
      <bpmn:incoming>Flow_25</bpmn:incoming>
      <bpmn:outgoing>Flow_26_Accessible</bpmn:outgoing>
      <bpmn:outgoing>Flow_27_NotAccessible</bpmn:outgoing>
    </bpmn:exclusiveGateway>

    <!-- Activity 8: Complete Compliance Training (APQC 4.1.8) -->
    <bpmn:task id="Activity_8_CompleteComplianceTraining" name="4.1.8 Complete Compliance Training">
      <bpmn:incoming>Flow_26_Accessible</bpmn:incoming>
      <bpmn:incoming>Flow_28</bpmn:incoming>
      <bpmn:outgoing>Flow_29</bpmn:outgoing>
    </bpmn:task>

    <!-- Loop back for IT issues -->
    <bpmn:sequenceFlow id="Flow_27_NotAccessible" name="No (15-20%)" sourceRef="Gateway_8_ITAccessStatus" targetRef="Activity_5_ProvisionITAccessEquipment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">IT Access Not Available</bpmn:conditionExpression>
    </bpmn:sequenceFlow>

    <bpmn:sequenceFlow id="Flow_28" sourceRef="Activity_5_ProvisionITAccessEquipment" targetRef="Activity_8_CompleteComplianceTraining" />

    <!-- Activity 9: Perform Check-Ins (APQC 4.1.9) -->
    <bpmn:task id="Activity_9_PerformCheckIns" name="4.1.9 Perform Check-Ins">
      <bpmn:incoming>Flow_29</bpmn:incoming>
      <bpmn:outgoing>Flow_30</bpmn:outgoing>
    </bpmn:task>

    <!-- End Events -->
    <bpmn:endEvent id="EndEvent_1" name="Onboarding Complete">
      <bpmn:incoming>Flow_30</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:endEvent id="EndEvent_OfferRescinded" name="Offer Rescinded">
      <bpmn:incoming>Flow_8_Rescind</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:endEvent id="EndEvent_ContractorOnboarding" name="Contractor Onboarding (Abbreviated)">
      <bpmn:incoming>Flow_11_Contractor</bpmn:incoming>
    </bpmn:endEvent>

    <!-- Sequence Flows -->
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Activity_1_InitiateNewHire" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Activity_1_InitiateNewHire" targetRef="Activity_2_ConductBackgroundCheck" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Activity_2_ConductBackgroundCheck" targetRef="Gateway_1_BackgroundResult" />
    
    <bpmn:sequenceFlow id="Flow_4_Clean" name="Clean (95%)" sourceRef="Gateway_1_BackgroundResult" targetRef="Activity_4_SetUpEmployeeRecord">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Background Check Clean</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_5_Issues" name="Issues Found (5%)" sourceRef="Gateway_1_BackgroundResult" targetRef="Activity_3_ReviewBackgroundIssues">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Background Check Issues</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Activity_3_ReviewBackgroundIssues" targetRef="Gateway_4_BackgroundDecision" />
    
    <bpmn:sequenceFlow id="Flow_7_Proceed" name="Proceed" sourceRef="Gateway_4_BackgroundDecision" targetRef="Activity_4_SetUpEmployeeRecord">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Proceed with Hire</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_8_Rescind" name="Rescind Offer" sourceRef="Gateway_4_BackgroundDecision" targetRef="EndEvent_OfferRescinded">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Rescind Offer</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_9" sourceRef="Activity_4_SetUpEmployeeRecord" targetRef="Gateway_2_EmploymentType" />
    
    <bpmn:sequenceFlow id="Flow_10_FullTime" name="Full-Time" sourceRef="Gateway_2_EmploymentType" targetRef="Activity_5_ProvisionITAccessEquipment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Full-Time Employee</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_11_Contractor" name="Contractor" sourceRef="Gateway_2_EmploymentType" targetRef="EndEvent_ContractorOnboarding">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Contractor</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_12" sourceRef="Activity_5_ProvisionITAccessEquipment" targetRef="Gateway_3_WorkLocation" />
    
    <bpmn:sequenceFlow id="Flow_13_Remote" name="Remote" sourceRef="Gateway_3_WorkLocation" targetRef="Activity_5a_ProvisionRemoteEquipment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Remote Employee</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_14_Onsite" name="On-site" sourceRef="Gateway_3_WorkLocation" targetRef="Activity_5b_ProvisionOnsiteEquipment">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">On-site Employee</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_15" sourceRef="Activity_5a_ProvisionRemoteEquipment" targetRef="Gateway_5_WorkspaceJoin" />
    <bpmn:sequenceFlow id="Flow_16" sourceRef="Activity_5b_ProvisionOnsiteEquipment" targetRef="Activity_6_AssignWorkspace" />
    <bpmn:sequenceFlow id="Flow_17" sourceRef="Activity_6_AssignWorkspace" targetRef="Gateway_5_WorkspaceJoin" />
    <bpmn:sequenceFlow id="Flow_18" sourceRef="Gateway_5_WorkspaceJoin" targetRef="Gateway_6_OrientationAlignment" />
    
    <bpmn:sequenceFlow id="Flow_19_OrientationDay" name="Yes (50%)" sourceRef="Gateway_6_OrientationAlignment" targetRef="Activity_7_ConductOrientation">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Orientation Monday</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_20_NonOrientationDay" name="No (50%)" sourceRef="Gateway_6_OrientationAlignment" targetRef="Activity_7a_ManagerFirstDayOrientation">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Non-Orientation Day</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_21" sourceRef="Activity_7a_ManagerFirstDayOrientation" targetRef="Activity_7_ConductOrientation" />
    <bpmn:sequenceFlow id="Flow_22" sourceRef="Activity_7_ConductOrientation" targetRef="Gateway_7_PreChecklistComplete" />
    
    <bpmn:sequenceFlow id="Flow_23_Complete" name="Yes (50%)" sourceRef="Gateway_7_PreChecklistComplete" targetRef="Gateway_8_ITAccessStatus">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Checklist Complete</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_24_Incomplete" name="No (50%)" sourceRef="Gateway_7_PreChecklistComplete" targetRef="Activity_10_CompleteRemainingPaperwork">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">Checklist Incomplete</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_25" sourceRef="Activity_10_CompleteRemainingPaperwork" targetRef="Gateway_8_ITAccessStatus" />
    
    <bpmn:sequenceFlow id="Flow_26_Accessible" name="Yes (80-85%)" sourceRef="Gateway_8_ITAccessStatus" targetRef="Activity_8_CompleteComplianceTraining">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">IT Access Available</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_29" sourceRef="Activity_8_CompleteComplianceTraining" targetRef="Activity_9_PerformCheckIns" />
    <bpmn:sequenceFlow id="Flow_30" sourceRef="Activity_9_PerformCheckIns" targetRef="EndEvent_1" />

  </bpmn:process>

  <!-- Diagram Information -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="Process_EmployeeOnboarding">

      <!-- Lane Shapes -->
      <bpmndi:BPMNShape id="Lane_HRCoordinator_di" bpmnElement="Lane_HRCoordinator" isHorizontal="true">
        <dc:Bounds x="160" y="80" width="2400" height="250" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_HRDirector_di" bpmnElement="Lane_HRDirector" isHorizontal="true">
        <dc:Bounds x="160" y="330" width="2400" height="200" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_ITDepartment_di" bpmnElement="Lane_ITDepartment" isHorizontal="true">
        <dc:Bounds x="160" y="530" width="2400" height="200" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_Facilities_di" bpmnElement="Lane_Facilities" isHorizontal="true">
        <dc:Bounds x="160" y="730" width="2400" height="200" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_HiringManager_di" bpmnElement="Lane_HiringManager" isHorizontal="true">
        <dc:Bounds x="160" y="930" width="2400" height="200" />
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_NewHire_di" bpmnElement="Lane_NewHire" isHorizontal="true">
        <dc:Bounds x="160" y="1130" width="2400" height="200" />
      </bpmndi:BPMNShape>

      <!-- Start Event -->
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="220" y="187" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="200" y="230" width="76" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Activity 1: Initiate New Hire -->
      <bpmndi:BPMNShape id="Activity_1_InitiateNewHire_di" bpmnElement="Activity_1_InitiateNewHire">
        <dc:Bounds x="300" y="165" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- Activity 2: Conduct Background Check -->
      <bpmndi:BPMNShape id="Activity_2_ConductBackgroundCheck_di" bpmnElement="Activity_2_ConductBackgroundCheck">
        <dc:Bounds x="440" y="165" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- Gateway 1: Background Result -->
      <bpmndi:BPMNShape id="Gateway_1_BackgroundResult_di" bpmnElement="Gateway_1_BackgroundResult" isMarkerVisible="true">
        <dc:Bounds x="580" y="180" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="563" y="150" width="84" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Activity 3: Review Background Issues -->
      <bpmndi:BPMNShape id="Activity_3_ReviewBackgroundIssues_di" bpmnElement="Activity_3_ReviewBackgroundIssues">
        <dc:Bounds x="555" y="380" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- Gateway 4: Background Decision -->
      <bpmndi:BPMNShape id="Gateway_4_BackgroundDecision_di" bpmnElement="Gateway_4_BackgroundDecision" isMarkerVisible="true">
        <dc:Bounds x="705" y="395" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="688" y="365" width="84" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Activity 4: Set Up Employee Record -->
      <bpmndi:BPMNShape id="Activity_4_SetUpEmployeeRecord_di" bpmnElement="Activity_4_SetUpEmployeeRecord">
        <dc:Bounds x="680" y="165" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- Gateway 2: Employment Type -->
      <bpmndi:BPMNShape id="Gateway_2_EmploymentType_di" bpmnElement="Gateway_2_EmploymentType" isMarkerVisible="true">
        <dc:Bounds x="820" y="180" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="803" y="150" width="84" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Activity 5: Provision IT Access and Equipment -->
      <bpmndi:BPMNShape id="Activity_5_ProvisionITAccessEquipment_di" bpmnElement="Activity_5_ProvisionITAccessEquipment">
        <dc:Bounds x="920" y="165" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- Gateway 3: Work Location -->
      <bpmndi:BPMNShape id="Gateway_3_WorkLocation_di" bpmnElement="Gateway_3_WorkLocation" isMarkerVisible="true">
        <dc:Bounds x="1060" y="180" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1043" y="150" width="84" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Activity 5a: Provision Remote Equipment -->
      <bpmndi:BPMNShape id="Activity_5a_ProvisionRemoteEquipment_di" bpmnElement="Activity_5a_ProvisionRemoteEquipment">
        <dc:Bounds x="1035" y="580" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- Activity 5b: Provision Onsite Equipment -->
      <bpmndi:BPMNShape id="Activity_5b_ProvisionOnsiteEquipment_di" bpmnElement="Activity_5b_ProvisionOnsiteEquipment">
        <dc:Bounds x="1160" y="580" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- Activity 6: Assign Workspace -->
      <bpmndi:BPMNShape id="Activity_6_AssignWorkspace_di" bpmnElement="Activity_6_AssignWorkspace">
        <dc:Bounds x="1160" y="780" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- Gateway 5: Workspace Join -->
      <bpmndi:BPMNShape id="Gateway_5_WorkspaceJoin_di" bpmnElement="Gateway_5_WorkspaceJoin" isMarkerVisible="true">
        <dc:Bounds x="1185" y="795" width="50" height="50" />
      </bpmndi:BPMNShape>

      <!-- Gateway 6: Orientation Alignment -->
